{
  "name": "hotel-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "npm run build:widget && npm run build:standalone && cp -r dist-standalone/* dist/",
    "build:widget": "vite build",
    "build:standalone": "vite build --config vite.config.standalone.ts",
    "preview": "vite preview --port 3000",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "preact": "^10.25.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.9.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-css-injected-by-js": "^3.5.0"
  }
}
